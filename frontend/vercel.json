{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm install -g npm@11.1.0 && CI=false GENERATE_SOURCEMAP=false npm run build",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "devCommand": "npm run start",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/socialRise.png",
      "dest": "/socialRise.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}